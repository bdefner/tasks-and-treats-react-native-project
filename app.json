{
  "expo": {
    "name": "Tasks-and-Treats",
    "slug": "tasks-and-treats",
    "description": "A get-things-done app, that's fun to use. Give yourself tasks and trade them for your treats :) ",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beppino.tasks-and-treats"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.beppino.tasksandtreats"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
